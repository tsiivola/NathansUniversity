start =
    expression

expression =
    atom
  / "(" whitespace? e:exps whitespace? ")" { return e; }
    
exps = 
    f:expression whitespace r:exps { return [f].concat(r); }
  / f:expression { return [f]; }

validchar
    = [0-9a-zA-Z_?!+-=@#$%^&*/.]
    
atom =
    chars:validchar+
        { return chars.join(""); }

whitespace = [\n\t ]+
